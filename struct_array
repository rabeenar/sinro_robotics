#include <stdio.h>
struct college
{
char name[5];
int staff;
int student;
};
int main() {
    struct college department[5];
    for(int i=0;i<=1;i++)
    {
        scanf("%s",&department[i].name);
        scanf("%d",&department[i].staff);
        scanf("%d",&department[i].student);
    }
    for(int i=0;i<=1;i++)
    {
        printf("%s\n",department[i].name);
        printf("%d\n",department[i].staff);
        printf("%d\n",department[i].student);
    }
    return 0;
}
